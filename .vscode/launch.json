{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "debug Client",
      "request": "launch",
      "type": "chrome",
      "url": "http://localhost:5173",
      "webRoot": "${workspaceFolder}",
      "port": 5173,
      "userDataDir": false
      
    },
    {
			"name": "debug Server",
			"type": "node-terminal",
			"request": "launch",
			"command": "pnpm run dev",
			"cwd": "${workspaceFolder}"
		},
  ]
}
